
@{
    ViewBag.Title = "Tables";
}

<div class="content">
    <div class="category_image ">

        <div class="category_img w-100">
            <img src="https://res-3.cloudinary.com/dwpujv6in/image/upload/c_pad,dpr_2.0,f_auto,q_auto/v1/media/catalog/tmp/category/WebM2_CategoryLandingPages_Marquees_Dining_Nov18.jpg"
                 alt="" class="image w-100 ">
        </div>
        <div class="category_content">
            <h4 class="text-center">New Tables Designs</h4>
            <a href="~/CreatProductList/Category?type=7">
                <img src="~/Assest/img/InformationImg/Circula Large Coffee Table-頁面大圖.jpg"
                     alt="Accent And Coffee Tables" class="image w-100">
                <h6>Accent And Coffee Tables</h6>
            </a>
            <a href="~/CreatProductList/Category?type=8">
                <img src="~/Assest/img/InformationImg/Cant Desk-頁面大圖.jpg"
                     alt="Office-Desks And Tables" class="image w-100">
                <h6>Office-Desks And Tables</h6>
            </a>
            <a href="~/CreatProductList/Category?type=9">
                <img src="~/Assest/img/InformationImg/Stash Desk-kids-頁面大圖.jpg"
                     alt="Kids-Desk" class="image w-100">
                <h6>Kids-Desk</h6>
            </a>
        </div>

    </div>
    <div class="category_menu ">
        <div class="category_menu_content">
            <div class="home_link">
                <a href="#">Home</a>
                <span>| Modern Dining Room Furniture</span>
            </div>
            <h1>Tables</h1>
            <ul>
                <li>
                    @Html.ActionLink("Accent And Coffee Tables", "Category", "CreatProductList", new { type = 7 }, new { sortOrder = ViewBag.ResetParm })
                </li>
                <li>
                    @Html.ActionLink("Office-Desks And Tables", "Category", "CreatProductList", new { type = 8 }, new { sortOrder = ViewBag.ResetParm })
                </li>
                <li>
                    @Html.ActionLink("Kids-Desk", "Category", "CreatProductList", new { type = 9 }, new { sortOrder = ViewBag.ResetParm })
                </li>
            </ul>
        </div>
    </div>
</div>

