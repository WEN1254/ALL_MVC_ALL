
@{
    ViewBag.Title = "CabinetAndDressingtable";
}

<div class="content">
    <div class="category_image ">
        <div class="category_img w-100">
            <img src="~/Assest/img/InformationImg/Chicago 8 Box-頁面大圖.jpg"
                 alt="" class="image w-100 ">
        </div>
        <div class="category_content">
            <h4 class="text-center">Cabinet And Dressingtable Designs</h4>
            <a href="~/CreatProductList/Category?type=12">
                <img src="~/Assest/img/InformationImg/Lap 2 Door  Credenza-頁面大圖.jpg"
                     alt="Dressers" class="image w-100">
                <h6>Dressers</h6>
            </a>
            <a href="~/CreatProductList/Category?type=13">
                <img src="~/Assest/img/InformationImg/Browser Low Bookcase-頁面大圖.jpg"
                     alt="Bookcases" class="image w-100">
                <h6>Bookcases</h6>
            </a>
        </div>

    </div>
    <div class="category_menu ">
        <div class="category_menu_content">
            <div class="home_link">
                <a href="#">Home</a>
                <span>| Modern Office Furniture</span>
            </div>
            <h1>Cabinet And Dressingtable</h1>
            <ul>
                <li>
                    @Html.ActionLink("Dressers", "Category", "CreatProductList", new { type = 12 }, new { sortOrder = ViewBag.ResetParm })
                </li>
                <li>
                    @Html.ActionLink("Bookcases", "Category", "CreatProductList", new { type = 13 }, new { sortOrder = ViewBag.ResetParm })
                </li>
            </ul>
        </div>
    </div>
</div>
@section topCSS{
    <link href="~/Assest/css/Introduction.css" rel="stylesheet" />
    <style>
    </style>
}
